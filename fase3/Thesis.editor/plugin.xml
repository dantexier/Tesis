<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated RDBMS -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="RDBMS.presentation.RDBMSModelWizardID"
            name="%_UI_RDBMSModelWizard_label"
            class="RDBMS.presentation.RDBMSModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RDBMSModelFile.gif">
         <description>%_UI_RDBMSModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated RDBMS -->
      <editor
            id="RDBMS.presentation.RDBMSEditorID"
            name="%_UI_RDBMSEditor_label"
            icon="icons/full/obj16/RDBMSModelFile.gif"
            extensions="rdbms"
            class="RDBMS.presentation.RDBMSEditor"
            contributorClass="RDBMS.presentation.RDBMSActionBarContributor">
      </editor>
   </extension>

</plugin>
